{
   "name": "electricity-bill-calculator--electron",
   "version": "1.0.0",
   "description": "A simple desktop application that allows user to calculate electricity bill",
   "main": "main.js",
   "scripts": {
      "test": "jest",
      "start": "electron .",
      "dev": "nodemon --exec electron .",
      "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electric Bill Calculator\"",
      "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds",
      "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/rpsbobby/electricity-bill-calculator--electron.js.git"
   },
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/rpsbobby/electricity-bill-calculator--electron.js/issues"
   },
   "homepage": "https://github.com/rpsbobby/electricity-bill-calculator--electron.js#readme",
   "dependencies": {
      "bootstrap": "^5.2.0-beta1",
      "electron": "^18.2.3"
   },
   "devDependencies": {
      "electron-packager": "^15.5.1",
      "jest": "^28.1.0",
      "nodemon": "^2.0.16"
   }
}
